{
  "expo": {
    "name": "Scott Club",
    "slug": "scott-club",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "scott-club",

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.seuusuario.scott-club"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.seuusuario.scott-club",
      "web": {
        "favicon": "./assets/favicon.png"
      },

      "plugins": [
        [
          "expo-auth-session",
          {
            "providers": ["google"]
          }
        ]
      ]
    }
  }
}
